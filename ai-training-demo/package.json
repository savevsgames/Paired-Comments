{
  "name": "paired-comments-ai-training-demo",
  "version": "0.1.0",
  "description": "AI Training Comparison Demo - Prove .comments improve model training",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "evaluate": "tsx src/evaluation/cli.ts",
    "compare": "tsx src/evaluation/compare.ts",
    "dashboard:dev": "cd src/dashboard && npm run dev",
    "dashboard:build": "cd src/dashboard && npm run build",
    "dashboard:start": "cd src/dashboard && npm run start"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "openai": "^4.20.0",
    "simple-statistics": "^7.8.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai-training",
    "code-generation",
    "machine-learning",
    "azure-ml",
    "fine-tuning"
  ],
  "author": "Paired Comments Team",
  "license": "MIT"
}
